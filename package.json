{
  "name": "@nuxtjs/partytown",
  "version": "1.1.0",
  "license": "MIT",
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "type": "module",
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "yarn build --stub && nuxi prepare playground",
    "prepack": "yarn build",
    "lint": "eslint --fix --ext .js,.ts,.vue . && prettier --write '**/*.{js,ts,md,vue}'",
    "test": "vitest run"
  },
  "dependencies": {
    "@builder.io/partytown": "^0.4.4",
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest",
    "knitwork": "^0.1.1",
    "serve-static": "^1.14.2",
    "ufo": "^0.7.11"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxt/test-utils": "npm:@nuxt/test-utils-edge@latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@release-it/conventional-changelog": "4.2.0",
    "@types/serve-static": "1.13.10",
    "c8": "^7.11.0",
    "eslint": "8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "nuxt3": "latest",
    "playwright": "^1.19.2",
    "prettier": "2.5.1",
    "release-it": "14.12.5",
    "vite": "^2.8.6",
    "vitest": "^0.6.0"
  }
}
